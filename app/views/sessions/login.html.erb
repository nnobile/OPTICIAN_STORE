<h1>Log In Here</h1>

<%= flash[:message]%>

<%= form_for @optician, url: '/login' do |f| %>
    
    <%= f.label :username %>
    <%= f.text_field :username %>
    </br></br>
    
    <%= f.label :password %>
    <%= f.password_field :password %>
    </br></br></br>
    
    <%= f.submit "Log In" %>
    </div></br>
    
<% end %>