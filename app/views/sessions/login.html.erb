<h1>Log In Here</h1>

<%= form_for(@optician) do |f| %>
    
    <div><br>
    <%= f.label :username %>
    <%= f.text_field :username %>
    </div></br>
    
    <div><br>
    <%= f.label :password %>
    <%= f.password_field :password %>
    </div></br>
    
    <div><br>
    <%= f.submit "Log In" %>
    </div></br>
    
<% end %>