<% if logged_in? %><br><br>
<h1> <%= flash[:message] = "Already logged in. Please log out or click another link above." %> </h1> <br>

<% else %>
<h1>Log In Here</h1>
<%= form_for @optician, url: '/login' do |f| %>
    
    <%= f.label :username %>
    <%= f.text_field :username %>
    </br></br>
    
    <%= f.label :password %>
    <%= f.password_field :password %>
    </br></br></br>
    
    <%= f.submit "Log In" %>
    </div></br>
    <% end %> 
<% end %>
